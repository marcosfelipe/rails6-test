<%= form_with(model: high_score, local: true) do%>

  <%= react_component 'TextInput', name: 'high_score[game]', label: :game, error: high_score.errors[:game].join(',') %>
  <%= react_component 'TextInput', name: 'high_score[score]', type: :number, label: :score, error: high_score.errors[:score].join('.') %>

  <%= react_component 'SubmitInput', name: :submit, id: :submit %>
<% end %>
